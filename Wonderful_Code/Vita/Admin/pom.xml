<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.agu</groupId>
	<artifactId>admin</artifactId>
	<packaging>jar</packaging>
	<version>1.0.0</version>
	<name>后台管理模块</name>
	<description>Vita后台管理模块</description>

	<parent>
		<groupId>com.agu</groupId>
		<artifactId>vita</artifactId>
		<version>1.0.0</version>
	</parent>

	<dependencies>
		<dependency>
			<groupId>com.agu</groupId>
			<artifactId>Common</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.agu</groupId>
			<artifactId>DevModule</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.agu.module</groupId>
			<artifactId>system</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.agu.component</groupId>
			<artifactId>shrio</artifactId>
			<version>${project.version}</version>
		</dependency>

		<dependency>
			<groupId>com.agu.component</groupId>
			<artifactId>actionLog</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.agu.component</groupId>
			<artifactId>thymeleaf</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.agu.component</groupId>
			<artifactId>fielUpload</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<excludes>
						<exclude>
							<groupId>org.projectlombok</groupId>
							<artifactId>lombok</artifactId>
						</exclude>
					</excludes>
				</configuration>
			</plugin>
		</plugins>
		<finalName>Vita-${project.version}</finalName>
	</build>

</project>
